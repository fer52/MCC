<?xml version='1.0' encoding='utf-8'?>
<widget id="com.websolutionsgt.mcc" version="1.1.3" versionCode="10" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Managment Collection Credits</name>
    <description>
        Administraci√≥n de creditos
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        FC
    </author>

    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <platform name="android">
        <icon height="180" src="www/img/icon.png" width="180" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-file" spec="6.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-dialogs" spec="2.0.1" />
    <plugin name="cordova-plugin-geolocation" spec="4.0.1" />
    <plugin name="cordova-plugin-device" spec="2.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="5.0.2" />
    <plugin name="cordova-plugin-android-permissions" spec="1.0.0" />
    <plugin name="cordova-plugin-appversion" spec="^1.0.0" />
    <engine name="android" spec="^7.1.1" />
</widget>
